$('.file').html("<%= j render partial: 'disasm' %>");
$('#org').val(<%= @org %>);
$('#file_size').html(<%= @file_size %> + ' bytes');
$('#log').empty();

var editor = ace.edit("textarea");
editor.setOptions({
  mode: "ace-builds/src/mode-assembly_x86",
  theme: "ace-builds/src/monokai",
  maxLines: 50,
  minLines: 20,
  fontSize: 12,
  autoScrollEditorIntoView: true,
  copyWithEmptySelection: true,
});
